<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="TOP_CENTER" prefHeight="400.0" prefWidth="300.0" spacing="8.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.swarm.dairy.yairy.control.controllers.MainController">
    <padding>
        <Insets bottom="12.0" left="12.0" right="12.0" top="12.0" />
    </padding>
   <children>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <MenuBar prefHeight="27.0" prefWidth="80.0">
              <menus>
                <Menu fx:id="actions" mnemonicParsing="false" text="Actions" />
              </menus>
            </MenuBar>
            <Button fx:id="undo" mnemonicParsing="false" text="&lt;" />
            <Button fx:id="make" mnemonicParsing="false" text="&gt;" />
            <Separator prefHeight="0.0" prefWidth="20.0" />
            <MenuBar>
              <menus>
                  <Menu mnemonicParsing="false" text="Exit">
                    <items>
                        <MenuItem fx:id="exitWithoutSaving" mnemonicParsing="false" text="Exit without saving" />
                        <MenuItem fx:id="reloadFile" mnemonicParsing="false" text="Reload file(cancel all changes)" />
                        <MenuItem fx:id="saveChanges" mnemonicParsing="false" text="Save changes" />
                      <MenuItem fx:id="exit" mnemonicParsing="false" text="Exit" />
                    </items>
                  </Menu>
                <Menu mnemonicParsing="false" text="File">
                  <items>
                        <MenuItem fx:id="update" mnemonicParsing="false" text="Update" />
                        <MenuItem fx:id="export" mnemonicParsing="false" text="Export" />
                        <MenuItem fx:id="add" mnemonicParsing="false" text="Add" />
                    <MenuItem fx:id="remove" mnemonicParsing="false" text="Remove" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Edit">
                  <items>
                    <MenuItem fx:id="delete" mnemonicParsing="false" text="Delete" />
                        <MenuItem fx:id="replace" mnemonicParsing="false" text="Replace" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                  <items>
                    <MenuItem fx:id="about" mnemonicParsing="false" text="About" />
                        <MenuItem fx:id="tutorial" mnemonicParsing="false" text="Tutorial" />
                  </items>
                </Menu>
                  <Menu mnemonicParsing="false" text="Security">
                    <items>
                      <MenuItem fx:id="passwords" mnemonicParsing="false" text="Passwords" />
                        <MenuItem fx:id="metadata" mnemonicParsing="false" text="Metadata" />
                        <MenuItem fx:id="setFakedText" mnemonicParsing="false" text="Set faked text" />
                        <MenuItem fx:id="setPassword" mnemonicParsing="false" text="Set password" />
                        <MenuItem fx:id="enterPassword" mnemonicParsing="false" text="Enter password" />
                    </items>
                  </Menu>
              </menus>
            </MenuBar>
         </children>
      </HBox>
      <TextArea fx:id="area" depthTest="ENABLE" prefHeight="776.0" prefWidth="280.0" wrapText="true">
         <font>
            <Font size="14.0" />
         </font>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </TextArea>
      <HBox alignment="BOTTOM_CENTER" prefHeight="20.0" prefWidth="260.0">
         <children>
            <Button fx:id="prevPage" mnemonicParsing="false" text="previous" textAlignment="CENTER">
               <font>
                  <Font size="12.0" />
               </font>
               <HBox.margin>
                  <Insets left="10.0" right="10.0" />
               </HBox.margin></Button>
            <Label fx:id="pagesNumber" prefHeight="29.0" prefWidth="31.0" text="page" textAlignment="CENTER" />
            <Button fx:id="nextPage" mnemonicParsing="false" text="next" textAlignment="CENTER">
               <font>
                  <Font size="12.0" />
               </font>
               <HBox.margin>
                  <Insets left="10.0" right="10.0" />
               </HBox.margin></Button>
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin></HBox>
   </children>

    
</VBox>
